pipeline {
    agent any
    stages {
        stage('compile stage') {
            steps {
                maven(maven : 'maven_3.6.0'){
                bat 'mvn clean compile'
            }
         }
     }
     stage('Testing stage'){
        steps {
           maven(maven : 'maven_3.6.0'){
                bat 'mvn test'
            }
         }
     }
      stage('Deployment stage'){
        steps {
            maven(maven : 'maven_3.6.0'){
                bat 'mvn deploy'
             }
         }
     }
   }
}
